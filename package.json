{
  "name": "batch-paper",
  "version": "1.0.0",
  "description": "CLI tool for batch image processing",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "package": "pkg ."
  },
  "pkg": {
    "targets": [
      "node20-macos-x64",
      "node20-win-x64"
    ],
    "outputPath": "executables"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}